<Window x:Class="Wpf2chdownloader.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wpf2chdownloader"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="602.309">
    <Grid>
        <TextBox x:Name="inputUrlBox" HorizontalAlignment="Left" Height="23" Margin="68,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="414"/>
        <TextBlock x:Name="inputUrlBlock" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="23" Width="53" Text="url треда"/>
        <Button x:Name="loadButton" Content="Load" HorizontalAlignment="Left" Margin="487,10,0,0" VerticalAlignment="Top" Width="90" Height="23" Click="loadButton_Click"/>
        <ComboBox x:Name="typeFileComboBox" HorizontalAlignment="Left" Margin="362,38,0,0" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="downloadButton" Content="Download" HorizontalAlignment="Left" Margin="487,38,0,0" VerticalAlignment="Top" Width="90" Height="22" Click="downloadButton_Click" IsEnabled="False"/>
        <TextBlock x:Name="infoBlock" HorizontalAlignment="Left" Margin="10,38,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="22" Width="347"/>
        <ProgressBar x:Name="progressBar" HorizontalAlignment="Left" Height="22" Margin="362,65,0,0" VerticalAlignment="Top" Width="215"/>
        <Button x:Name="md5Button" Content="CreateMD5Table" HorizontalAlignment="Left" Margin="487,92,0,0" VerticalAlignment="Top" Width="90" Height="22" FontSize="10" Click="md5Button_Click"/>

    </Grid>
</Window>
